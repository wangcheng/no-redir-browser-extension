{
  "name": "No Redirect",
  "version": "1.0",
  "description": "No Redirect",
  "background": {
    "persistent": true,
    "scripts": ["js/background.js"]
  },
  "icons": {
    "16": "img/icon_awesome_face_16.png",
    "32": "img/icon_awesome_face_32.png",
    "48": "img/icon_awesome_face_48.png",
    "128": "img/icon_awesome_face_128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "storage",
    "notifications",
    "<all_urls>"
  ],
  "manifest_version": 2
}
