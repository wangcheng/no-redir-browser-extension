{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "es2015",
    "target": "es2015",
    "jsx": "react",
    "jsxFactory": "Snabbdom.createElement",
    "sourceMap": true,
    "moduleResolution": "node",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "strictNullChecks": true
  },
  "include": ["src"]
}
